<!DOCTYPE html>
<html lang="en">
{{> baseHead customTitle=customTitle }}
<body>
    {{> siteHeader }}
    {{#if onePost}}
        <div style="background-color: rgb(231, 169, 169);">
            <h2>{{onePost.title}}</h2>
            <p>
                <span>{{ onePost.from }}</span>
                <span>{{ onePost.createdAtText }}</span>
            </p>
        </div>
    {{else}}
    {{/if}}
    {{> siteFooter }}
</body>
</html>